mixin actions(id = '', baseUrl = '')
    td 
        a.action(href=baseUrl + 'edit/' + id)
            i.fa.fa-duotone.fa-pen-to-square
        a.action(href=baseUrl + 'duplicate/' + id)
            i.fa.fa-duotone.fa-copy
        a.action(onclick="deleteItem('" + baseUrl + "'," + id + ")")
            i.fa.fa-duotone.fa-eraser